import UIKit

class HWServiceProductModel: NSObject
{
    //服务产品id
    let serviceProductId : NSString?
    
    //机会类型id
    let chanceTypeId : NSString?
    
    //产品描述
    let productDescription : NSString?
    
    //产品介绍
    let productIntro : NSString?
    
    //机会类型
    let chanceType : NSString?
    
    //产品名称
    let productName : NSString?
    
    //图片url
    let imageURL : NSString?
    
    //详情Url
    let detailsURL : NSString?

    init(dic:NSDictionary)
    {
        self.serviceProductId = dic.stringObjectForKey("serviceProductId")
        self.chanceTypeId = dic.stringObjectForKey("chanceTypeId")
        self.productDescription = dic.stringObjectForKey("productDescription")
        self.productIntro = dic.stringObjectForKey("productIntro")
        self.chanceType = dic.stringObjectForKey("chanceType")
        self.productName = dic.stringObjectForKey("productName")
        self.imageURL = dic.stringObjectForKey("imageURL")
        self.detailsURL = dic.stringObjectForKey("detailsURL")
    }
}
